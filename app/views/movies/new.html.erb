<h1>Welcome :)</h1>

<h2>Create a New Movie </h2>
  
  <%= form_for @movie do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <div>
    <%= f.label :year_released %>
    <%= f.number_field :year_released %>
  </div>

 	<b><%= submit_tag "Submit" %></b>
<% end %>
 
